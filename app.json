{
  "expo": {
    "name": "Pledge - Bet On Yourself",
    "scheme": "pledge",
    "slug": "pledge",
    "version": "1.2.2",
    "plugins": ["expo-apple-authentication"],
    "orientation": "portrait",
    "icon": "./assets/logos/1024ios.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.cewidiupleek.pledge",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera so Stripe can scan your card information."
      },
      "buildNumber": "23",
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.cewidiupleek.pledge"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg",
        "cjs"
      ]
    },
    "owner": "cewidiupleek",
    "extra": {
      "eas": {
        "projectId": "4d655c1e-0a69-4e8d-9502-2cd45c722f4d"
      }
    }
  }
}
