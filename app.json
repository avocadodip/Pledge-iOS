{
  "expo": {
    "name": "Pledge - Bet On Yourself",
    "slug": "pledge",
    "plugins": [
      "@react-native-google-signin/google-signin"
    ],
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/logos/1024ios.png",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cewidiupleek.pledge",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera so Stripe can scan your card information."
      },
      "buildNumber": "14"
    },
    "android": {
      "package": "com.cewidiupleek.pledge"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "owner": "cewidiupleek",
    "extra": {
      "eas": {
        "projectId": "4d655c1e-0a69-4e8d-9502-2cd45c722f4d"
      }
    }
  }
}
